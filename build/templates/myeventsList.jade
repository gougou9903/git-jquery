each item in items
    div.row 
        div.three.columns
            //a(href="#", onClick='myRepo("' + item.full_name + '")')
            a(href="#")
                = item.id
        div.nine.columns(align = 'right')
          div.row
            | Type: 
            = item.type
          div.row
            | Repo Name: 
            = item.repo['name']
          div.row
            | Public: 
            = item.public
          div.row
            | Created at:  
            = item.created_at